void main() {
	auto ip = readAs!(int[]), N = ip[0], H = ip[1], M = ip[2], T = ip[3];
	int tmp = H * 60 + M;
	tmp += (N - 1) * T;
	tmp %= 1440;
	H = tmp / 60;
	M = tmp % 60;
	H.writeln;
	M.writeln;
}

