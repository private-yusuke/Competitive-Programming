void main() {
	auto N = ri;
	auto M = ri;
	auto A = readAs!(int[]);
	int[] scores = new int[](N);
	foreach(i; 0..M) {
		auto B = readAs!(int[]);
		auto T = A[i];
		scores[T-1] += B.filter!(i => i != T).array.length;
		scores[T-1]++;
		foreach(k, v; B) {
			if(k+1 == T) continue;
			if(v == T) scores[k]++;
		}
	}
	scores.each!writeln;
}

