void main() {
	auto ip = readAs!(int[]), D = ip[0], N = ip[1];
	switch(D) {
		case 0:
			if(N == 100) writeln(101);
			else writeln(N);
			break;
		case 1:
			if(N == 100) writeln(10100);
			else writeln(N*100);
			break;
		case 2:
			if(N == 100) writeln(1010000);
			else writeln(N*10000);
			break;
		default: break;
	}
}

